<p id="notice"><%= notice %></p>

<h1>Listing Ocs</h1>

<table>
  <thead>
    <tr>
      <th>Oc</th>
      <th>Estados</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ocs.each do |oc| %>
      <tr>
        <td><%= oc.oc %></td>
        <td><%= oc.estados %></td>
        <td><%= link_to 'Show', oc %></td>
        <td><%= link_to 'Edit', edit_oc_path(oc) %></td>
        <td><%= link_to 'Destroy', oc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Oc', new_oc_path %>
